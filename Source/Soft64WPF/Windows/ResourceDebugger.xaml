<Window x:Class="Soft64WPF.Windows.ResourceDebugger"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Resouce Debugger" Height="342" Width="573">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="16*"/>
            <RowDefinition Height="295*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <StackPanel.Resources>
                <Style TargetType="CheckBox">

                </Style>
            </StackPanel.Resources>
            <CheckBox x:Name="xaml_CheckBoxCPUMem" Content="CPU VMemory I/O" Checked="xaml_CheckBoxCPUMem_Checked" />
        </StackPanel>
        <RichTextBox x:Name="xaml_ResLog" HorizontalAlignment="Left" Height="295" IsReadOnly="True" Grid.Row="1" VerticalAlignment="Top" Width="565">
        </RichTextBox>
    </Grid>
</Window>
