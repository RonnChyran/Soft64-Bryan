<Window x:Class="Soft64WPF.Windows.CodeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CodeWindow" Height="334" Width="518">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="11*" />
            <RowDefinition Height="65*" />
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <Button Content="Scan Code" HorizontalAlignment="Left" Margin="10,12,0,0" VerticalAlignment="Top" Width="75" Height="22" />
        </StackPanel>
        <RichTextBox Grid.Row="1" x:Name="xaml_AnalsysResult">
        </RichTextBox>
    </Grid>
</Window>